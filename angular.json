{
  "$schema": "../../../node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "fr-auth": {
      "root": "src",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser-esbuild",
          "options": {
            "outputPath": "../../../dist/fronts/fr-auth",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "./src/polyfills.ts",
            "tsConfig": "tsconfig.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "input": "../../../packages/solenopsys/uimatrix/ui-templates/src/assets",
                "glob": "**/*",
                "output": "assets"
              }
            ],
            "styles": [
              "../../../packages/solenopsys/uimatrix/ui-templates/src/assets/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "fr-auth:build",
            "proxyConfig": "./proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "fr-auth:build:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}